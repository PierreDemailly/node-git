{
  "extends": "@nodesecure/eslint-config",
  "parserOptions": {
      "sourceType": "module",
      "requireConfigFile": false
  },
  "rules": {
    "sort-imports": ["error", {
      "ignoreCase": false,
      "ignoreDeclarationSort": false,
      "ignoreMemberSort": false,
      "memberSyntaxSortOrder": ["none", "all", "multiple", "single"],
      "allowSeparatedGroups": false
    }]
  }
}
