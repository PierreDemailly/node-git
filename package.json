{
  "name": "@pierred/node-git",
  "version": "1.2.0",
  "description": "Git API for node.js",
  "main": "./index.js",
  "scripts": {
    "test": "node --test ./test",
    "test:c8": "c8 npm run test",
    "test:e2e": "glob -c \"node --test\" \"./test/**.e2e-test.js\"",
    "test:e2e:c8": "c8 npm run test:e2e",
    "test:all": "npm run test && npm run test:e2e",
    "test:all:c8": "c8 npm run test:all",
    "lint": "eslint src test",
    "docs": "typedoc index.d.ts",
    "pkg:ok": "npx pkg-ok"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PierreDemailly/node-git.git"
  },
  "keywords": [
    "git",
    "api"
  ],
  "author": "PierreDemailly <pierredemailly.pro@gmail.com>",
  "files": [
    "index.d.ts",
    "index.js",
    "src"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PierreDemailly/node-git/issues"
  },
  "homepage": "https://github.com/PierreDemailly/node-git#readme",
  "type": "module",
  "types": "index.d.ts",
  "devDependencies": {
    "@nodesecure/eslint-config": "^1.8.0",
    "@release-it/keep-a-changelog": "^4.0.0",
    "c8": "^8.0.1",
    "glob": "^10.3.10",
    "is-ci": "^3.0.1",
    "pkg-ok": "^3.0.0",
    "typedoc": "^0.25.2"
  },
  "dependencies": {
    "@topcli/prompts": "^1.4.0"
  }
